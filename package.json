{
  "name": "product-and-cart",
  "version": "1.0.0",
  "description": "This is one of the companion repos to the Vue.js course on the freeCodeCamp YouTube channel.\r You can see the different stages of development by checking out the other branches.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run sass\" \"npm run serve\"",
    "serve": "live-server --open=/src/app.html --ignore=.git,node_modules",
    "sass": "node-sass -i -w --output-style=compressed src/styles/style.scss src/styles/style.min.css",
    "lint": "node-sass -i --output-style=compressed src/styles/style.scss src/styles/style.min.css && npm run sasslint",
    "sasslint": "sass-lint -c .sass-lint.yml -v -f table"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "live-server": "^1.2.1",
    "node-sass": "^7.0.1",
    "normalize.css": "^8.0.1",
    "sass-lint": "^1.13.1",
    "tjw-sasslint-rules": "^2.1.0"
  },
  "keywords": [],
  "author": "TheJaredWilcurt/Gwendolyn Faraday",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlmasF/vue3-fcc-course-basic-product-cart-demo.git"
  },
  "bugs": {
    "url": "https://github.com/AlmasF/vue3-fcc-course-basic-product-cart-demo/issues"
  },
